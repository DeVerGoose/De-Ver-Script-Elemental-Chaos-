local function AbsoluteButton(Button)
	local pos = Button.AbsolutePosition

	local X = pos.X
	local Y = pos.Y
	
	
	return X, Y
end

local function TopFrameButtonSetup(Button)
	if Button == CurrentPage then return end
	UiClick:Play()
	CurrentPage = Button
	
	for _, v in PagesFolder:GetChildren()  do
		if v ~= Button.Name then
			
		end
	end
	
	if math.random(1, 2) == 1 then
		Button.Rotation = 15
	else
		Button.Rotation = -15
	end
	
	for _, v in TopFrameFolder:GetChildren() do
		if v:IsA("TextButton") and v ~= CloseButton and v ~= Button then
			TS:Create(v, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {TextColor3 = Color3.new(0.666667, 0.666667, 0.666667)}):Play()
		end
	end
	
	for _, v in PagesFolder:GetChildren() do
		if v:IsA("Frame") and v.Name ~= CurrentPage.Name then
			TS:Create(v, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Position = UDim2.new(0, 0, -1, 0)}):Play()
			
			task.delay(0.25, function()
				if v.Name ~= CurrentPage.Name  then
					v.Visible = false
				end
			end)
		elseif v:IsA("Frame") and v.Name == CurrentPage.Name then
			v.Position = UDim2.new(0, 0, 1, 0)
			v.Visible = true
			TS:Create(v, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Position = UDim2.new(0, 0, 0.2, 0)}):Play()
		end
	end
	
	TS:Create(Button, TweenInfo.new(0.15, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Rotation = 0}):Play()
	TS:Create(Button, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {TextColor3 = Color3.new(1, 1, 1)}):Play()
	TS:Create(SelectFrame, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = Button.Size, Position = Button.Position}):Play()
end

local function SwitcherSetup(Switcher)
	if Switcher:GetAttribute("Switch") == true then
		Switcher:SetAttribute("Switch", false)
		UiOff:Play()
		
		TS:Create(Switcher, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Position = UDim2.new(1.225, 0, 0.195, 0), BackgroundColor3 = Color3.new(0.545098, 0.545098, 0.545098)}):Play()
		TS:Create(Switcher.Parent, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {TextColor3 = Color3.new(0.545098, 0.545098, 0.545098)}):Play()
		TS:Create(Switcher.Parent.Background, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.new(0.0666667, 0.0666667, 0.0666667)}):Play()
	else
		Switcher:SetAttribute("Switch", true)
		UiOn:Play()
		
		TS:Create(Switcher, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Position = UDim2.new(1.55, 0, 0.195, 0), BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
		TS:Create(Switcher.Parent, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {TextColor3 = Color3.new(1, 1, 1)}):Play()
		TS:Create(Switcher.Parent.Background, TweenInfo.new(0.15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.new(0.8, 0.352941, 0.352941)}):Play()
	end
end

local function GetEnemy()
	local enemies = {}
	for _, v in workspace:GetChildren() do
		if v:IsA("Model") and v:FindFirstChild("Humanoid") and v ~= Character then
			local EnemyPlr = game.Players:GetPlayerFromCharacter(v)
			if EnemyPlr then
				local data = game.ReplicatedStorage.Data[EnemyPlr.UserId]
				if data and data.other.InMenu.Value == false then
					local hum = v:FindFirstChild("Humanoid")
					if hum and hum.Health > 0 then
						table.insert(enemies, {
							Model = v,
							Humanoid = hum,
							Health = hum.Health
						})
					end
				end
			end
		end
	end

	if #enemies > 0 then
		table.sort(enemies, function(a, b)
			return a.Health < b.Health
		end)
		CurrentTarget = enemies[1].Model
	else
		CurrentTarget = nil
	end
end

local function UseTool(toolName, delayTime)
	local Tool = Player.Backpack:FindFirstChild(toolName)
	if Tool then
		Tool.Parent = Player.Character
		Tool:Activate()
		task.delay(0.1, function()
			if Tool then
				Tool:Deactivate()
				Tool.Parent = Player.Backpack
			end
		end)
		if delayTime and delayTime > 0 then
			task.wait(delayTime)
		end
	end
end

local function RandomTeleport(waitTime)
	local TeleportPoss = math.random(1, #SaveAF_TP_List)
	HumRP.CFrame = SaveAK_TP_List[TeleportPoss]
	if waitTime and waitTime > 0 then
		task.wait(waitTime)
	end
end

local function AutoKill()
	HumRP.Anchored = false
	RandomTeleport(0)
	GetEnemy()

	if not (CurrentTarget and CurrentTarget:FindFirstChild("Humanoid")) then
		CurrentTarget = nil
		return
	end

	local targetHumanoid = CurrentTarget.Humanoid
	local EnemyPlr = game.Players:GetPlayerFromCharacter(CurrentTarget)

	if not (EnemyPlr and targetHumanoid.Health > 0 and game.ReplicatedStorage.Data[EnemyPlr.UserId].other.InMenu.Value == false) then
		CurrentTarget = nil
		return
	end

	for _, v in Player.Character:GetChildren() do
		if v:IsA("Tool") then
			v.Parent = Player.Backpack
		end
	end

	local element = game.ReplicatedStorage.Data[Player.UserId].Element.Value

	local start = tick()
	
	if element == "Water" then
		UseTool("Water Essene", 1)
		HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		UseTool("Splash", 1)
		UseTool("Water Drop", 1)
		RandomTeleport(0)
		UseTool("Water Essene", 10)
	elseif element == "Ice" then
		local targetHRP = CurrentTarget:FindFirstChild("HumanoidRootPart")
		if targetHRP then
			HumRP.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 10)

			local runService = game:GetService("RunService")
			local conn
			local start = tick()

			conn = runService.Heartbeat:Connect(function()
				if tick() - start > 1.75 or not CurrentTarget or not CurrentTarget:FindFirstChild("HumanoidRootPart") then
					conn:Disconnect()
					return
				end

				HumRP.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 10)
			end)

			UseTool("Cryoformation", 1.8)
			RandomTeleport(5)
		end

	elseif element == "Electricity" then
		UseTool("Electric Charge", 2)
		while tick() - start < 1 do
			if CurrentTarget:FindFirstChild("HumanoidRootPart") then
				HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame * CFrame.new(0, 2, -1.25)

			end
			task.wait(0.025)
		end

		UseTool("Thundercloud", 1)
		RandomTeleport(14)
	elseif element == "Darkness" then
		HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		UseTool("Shatter", 1)
		RandomTeleport(8)

	elseif element == "Blood" then
		local targetHRP = CurrentTarget:FindFirstChild("HumanoidRootPart")
		if targetHRP then
			HumRP.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 10)

			local runService = game:GetService("RunService")
			local conn
			local start = tick()

			conn = runService.Heartbeat:Connect(function()
				if tick() - start > 1 or not CurrentTarget or not CurrentTarget:FindFirstChild("HumanoidRootPart") then
					conn:Disconnect()
					return
				end

				HumRP.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 10)
			end)
			
			UseTool("Blood Saw", 1.1)
			RandomTeleport(2)
		end
		

	elseif element == "Cosmos" then
		HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		UseTool("Plunnet Summon", 1)
		if CurrentTarget:FindFirstChild("HumanoidRootPart") then
			HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		end
		UseTool("Pocket Nova", 1)
		RandomTeleport(14)

	elseif element == "Slime" then
		HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		UseTool("Splash", 1)
		if CurrentTarget:FindFirstChild("HumanoidRootPart") then
			HumRP.CFrame = CurrentTarget.HumanoidRootPart.CFrame
		end
		UseTool("Water Drop", 1)
		RandomTeleport(0)
		UseTool("Water Essene", 10)

	else
		Notification("Notification", "Auto Kill", "Havent combo for this Element.")
		SwitcherSetup(AutoKillSwitchButton)
	end

	task.wait(1)
	if AutoKillSwitchButton:GetAttribute("Switch") == true then
		AutoKill()
	end
end

local function AutoFarmFunction()
	HumRP.Anchored = false
	
	for _, v in Player.Character:GetChildren() do
		if v:IsA("Tool") then
			v.Parent = Player.Backpack
		end
	end
	
	local TeleportPoss = math.random(1, #SaveAF_TP_List)
	HumRP.CFrame = SaveAF_TP_List[TeleportPoss]
	
	task.delay(0.25, function()
		HumRP.Anchored = true
	end)
	
	for _, v in Player.Backpack:GetChildren() do
		v.Parent = Player.Character
		v:Activate()
		task.wait(0.25)
		v:Deactivate()
		v.Parent = Player.Backpack
		task.wait(0.75)
	end
	
	HumRP.Anchored = false
	if AutoFarmSwitchButton:GetAttribute("Switch") == true then
		AutoFarmFunction()
	end
end

local function toggleFly()
	flying = not flying
	if flying then
		Notification("Notification", "Simple Fly", "Fly ON")
		
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.Velocity = Vector3.zero
		bodyVelocity.MaxForce = Vector3.new(1e5, 1e5, 1e5)
		bodyVelocity.Parent = HumRP
		Humanoid.PlatformStand = true
	else
		Notification("Notification", "Simple Fly", "Fly OFF")
		
		if bodyVelocity then
			bodyVelocity:Destroy()
			bodyVelocity = nil
		end
		Humanoid.PlatformStand = false
	end
end

local function WallHackHL(Char)
	local WH_HL = Instance.new("Highlight", Char)
	WH_HL.FillTransparency = 1
	WH_HL.OutlineTransparency = 1
	TS:Create(WH_HL, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {OutlineTransparency = 0}):Play()
	WH_HL.OutlineColor = WHColor
	WH_HL.Name = "WallHackHL"
end

local function SmoothColorChangeWH(Char)
	local WH_HL = Char:FindFirstChild("WallHackHL")
	
	if WH_HL then
		TS:Create(WH_HL, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {OutlineColor = WHColor}):Play()
	end
end

local function hexToColor3(hex)
	hex = hex:gsub("#","")
	if #hex == 6 then
		local r = tonumber(hex:sub(1,2),16)/255
		local g = tonumber(hex:sub(3,4),16)/255
		local b = tonumber(hex:sub(5,6),16)/255
		return Color3.new(r,g,b)
	end
	return nil
end

WHTextBox:GetPropertyChangedSignal("Text"):Connect(function()
	local color = hexToColor3(WHTextBox.Text)
	if color then
		WHPredictColor.BackgroundColor3 = color
	end
end)

WHTextBox.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		local color = hexToColor3(WHTextBox.Text)
		if color then
			WHPredictColor.BackgroundColor3 = color
			WHColor = color
			WHColorButton.BackgroundColor3 = color
			
			UiNotificator:Play()
			Notification("Notification", "Wall Hack", "Color changed successfully!")
			
			for _, v in workspace:GetChildren() do
				if v:IsA("Model") and v:FindFirstChild("Humanoid") and v ~= Character then
					SmoothColorChangeWH(v)
				end
			end
		end
		
		WHColorButton:SetAttribute("Active", false)
		WHColorChooseFrame.Visible = false
	end
end)

for _, v in TopFrameFolder:GetChildren() do
	if v:IsA("TextButton") and v ~= CloseButton then
		v.MouseButton1Click:Connect(function()
			TopFrameButtonSetup(v)
		end)
	end
end

RNS.RenderStepped:Connect(function()
	if game.ReplicatedStorage:FindFirstChild("Data") then
		Element.Text = "Element: " .. game.ReplicatedStorage.Data[Player.UserId].Element.Value
	end

	if WHSwitchButton:GetAttribute("Switch") == true then
		for _, v in workspace:GetChildren() do
			if v:IsA("Model") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("WallHackHL") and v ~= Character then
				WallHackHL(v)
			end
		end
	end
	
	if AutoRespawnSwitchButton:GetAttribute("Switch") == true then
		if game.ReplicatedStorage.Data[Player.UserId].other.InMenu.Value == true then
			local Spawn_upvr = game.ReplicatedStorage.Remotes:WaitForChild("Spawn")
			Spawn_upvr:FireServer()
		end
	end
	
	if flying and bodyVelocity then
		local moveDir = Vector3.zero
		if UIS:IsKeyDown(Enum.KeyCode.W) then
			moveDir = moveDir + Camera.CFrame.LookVector
		end
		if UIS:IsKeyDown(Enum.KeyCode.S) then
			moveDir = moveDir - Camera.CFrame.LookVector
		end
		if UIS:IsKeyDown(Enum.KeyCode.A) then
			moveDir = moveDir - Camera.CFrame.RightVector
		end
		if UIS:IsKeyDown(Enum.KeyCode.D) then
			moveDir = moveDir + Camera.CFrame.RightVector
		end
		if UIS:IsKeyDown(Enum.KeyCode.Space) then
			moveDir = moveDir + Vector3.new(0,1,0)
		end
		if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then
			moveDir = moveDir - Vector3.new(0,1,0)
		end

		if moveDir.Magnitude > 0 then
			bodyVelocity.Velocity = moveDir.Unit * flySpeed
		else
			bodyVelocity.Velocity = Vector3.zero
		end
	end
end)

UIS.InputBegan:Connect(function(input, isProcessed)
	if isProcessed then return end
	
	if input.KeyCode == Enum.KeyCode.R then
		if TPSwitchButton:GetAttribute("Switch") == true then
			local targetPosition = Player:GetMouse().Hit.p + Vector3.new(0, 3, 0)
			HumRP.CFrame = CFrame.new(targetPosition)

			UiNotificator:Play()
			Notification("Notification", "Teleport", "Successfully teleported to the mouse position")
		end
	elseif input.KeyCode == Enum.KeyCode.G then
		SwitcherSetup(FlySwitchButton)
		toggleFly()
	end
end)

TPSwitchButton.MouseButton1Click:Connect(function()
	SwitcherSetup(TPSwitchButton)
end)

AutoFarmSwitchButton.MouseButton1Click:Connect(function()
	SwitcherSetup(AutoFarmSwitchButton)
	if AutoFarmSwitchButton:GetAttribute("Switch") == true then
		AutoFarmFunction()
	else
		HumRP.Anchored = false
	end
end)

AutoKillSwitchButton.MouseButton1Click:Connect(function()
	if game.ReplicatedStorage.Data[Player.UserId].Level.Value < 20 then
		Notification("Notification", "Auto Kill", "You need to be level 20 to use this feature")
	else
		SwitcherSetup(AutoKillSwitchButton)
		if AutoKillSwitchButton:GetAttribute("Switch") == true then
			Notification("Notification", "Auto Kill", "Auto Kill ON")

			AutoKill()
		else
			Notification("Notification", "Auto Kill", "Auto Kill OFF")
		end
	end
end)

AutoRespawnSwitchButton.MouseButton1Click:Connect(function()
	SwitcherSetup(AutoRespawnSwitchButton)
end)

FlySwitchButton.MouseButton1Click:Connect(function()
	SwitcherSetup(FlySwitchButton)
	toggleFly()
end)

WHColorButton.MouseButton1Click:Connect(function()
	if WHColorButton:GetAttribute("Active") == true then
		WHColorButton:SetAttribute("Active", false)
		WHColorChooseFrame.Visible = false
	else
		WHColorButton:SetAttribute("Active", true)
		WHColorChooseFrame.Visible = true
	end
end)

WHSwitchButton.MouseButton1Click:Connect(function()
	SwitcherSetup(WHSwitchButton)
	if WHSwitchButton:GetAttribute("Switch") == true then
		
		Notification("Notification", "Wall Hack", "Wall Hack ON")
		
		for _, v in workspace:GetChildren() do
			if v:IsA("Model") and v:FindFirstChild("Humanoid") and v ~= Character then
				WallHackHL(v)
			end
		end
	else
		
		Notification("Notification", "Wall Hack", "Wall Hack OFF")

		for _, v in workspace:GetDescendants() do
			if v:IsA("Highlight") and v.Name == "WallHackHL" then
				TS:Create(v, TweenInfo.new(0.25, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {OutlineTransparency = 1}):Play()
				task.delay(0.25, function()
					v:Destroy()
				end)
			end
		end
	end
end)

CloseButton.MouseButton1Click:Connect(function()
	UiClose:Play()
	CloseButton.Interactable = false
	TS:Create(Background, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 0, 0, 0)}):Play()
	
	task.delay(0.2, function()
		ButtonTag.Position = Background.Position
		ButtonTag.Visible = true
		Background.Visible = false
		ButtonTag.Interactable = true
	end)
end)

ButtonTag.MouseButton1Click:Connect(function()
	UiOpen:Play()
	ButtonTag.Interactable = false
	TS:Create(Background, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0.277, 0, 0.282, 0)}):Play()
	Background.Visible = true
	CloseButton.Interactable = true
	
	task.delay(0.25, function()
		ButtonTag.Visible = false
	end)
end)

-- Character Reseting
Player.CharacterAdded:Connect(function(char)
	Character = char
	Humanoid = char:WaitForChild("Humanoid")
	HumRP = char:WaitForChild("HumanoidRootPart")
	
	print("ehh..")
	if FlySwitchButton:GetAttribute("Switch") == true then
		SwitcherSetup(FlySwitchButton)
		toggleFly()
	end
end)
